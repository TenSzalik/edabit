//Create a function that flips M's to W's (all uppercase).
using System;
using NUnit.Framework;

[TestFixture]
public class Tests
{
    [Test]
		[TestCase("WHAT DO YOU MEAN WE'RE OUT OF MONEY", Result =  "WHAT DO YOU WEAN WE'RE OUT OF WONEY")]
