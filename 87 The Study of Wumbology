 		[TestCase("1 WUMBO 2 WUMBO 3 WUMBO 4", Result =  "1 WUWBO 2 WUWBO 3 WUWBO 4")]
    public static string FixedTest(string a)
    {
        return Program.Wumbo(a);
    }
}
